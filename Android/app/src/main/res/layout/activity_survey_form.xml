<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.survey.SurveyFormActivity"
    android:background="#ffffff">

    <TextView
        android:id="@+id/SurveyForm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:text="Survey Form"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

            <include
                android:id="@+id/frameLayout_sgpa"
                layout="@layout/fragment_sgpa"
                android:layout_width="match_parent"
                android:layout_height="500dp"
                android:layout_marginTop="19dp"
                android:orientation="vertical"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/SurveyForm"
                app:layout_constraintVertical_bias="0.0">

            </include>

            <include
        layout="@layout/fragment_skillset"
        android:id="@+id/frameLayout_skillSet"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:layout_marginTop="19dp"
        android:orientation="vertical"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintTop_toBottomOf="@+id/SurveyForm">

    </include>

    <include
        android:id="@+id/frameLayout_package"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:layout_marginTop="19dp"
        android:orientation="vertical"
        layout="@layout/fragment_package"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SurveyForm">

    </include>


    <include
        android:id="@+id/frameLayout_domain"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:layout_marginTop="19dp"
        android:orientation="vertical"
        layout="@layout/fragment_domain"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SurveyForm">

    </include>

    <include
        android:id="@+id/frameLayout_job"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:layout_marginTop="19dp"
        android:orientation="vertical"
        layout="@layout/fragment_job"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SurveyForm">

    </include>

    <include
        android:id="@+id/frameLayout_learning1"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:layout_marginTop="19dp"
        android:orientation="vertical"
        layout="@layout/fragment_learning1"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SurveyForm">

    </include>

    <include
        android:id="@+id/frameLayout_learning2"
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:layout_marginTop="19dp"
        android:orientation="vertical"
        layout="@layout/fragment_learning2"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SurveyForm">

    </include>


    <ImageView
        android:id="@+id/back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:src="@drawable/ic_back"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.944" />

    <ImageView
        android:id="@+id/next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:src="@drawable/ic_next"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.945" />



</androidx.constraintlayout.widget.ConstraintLayout>